{
  "name": "tsd-lite",
  "version": "0.5.4",
  "description": "Test your TypeScript types easily",
  "license": "MIT",
  "repository": "https://github.com/mrazauskas/tsd-lite.git",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "build": "tsc",
    "lint": "yarn lint:cspell && yarn lint:eslint && yarn lint:prettier",
    "lint:cspell": "cspell --relative --show-context --show-suggestions",
    "lint:eslint": "eslint . --ext .ts",
    "lint:prettier": "prettier . --write",
    "test": "jest"
  },
  "files": [
    "build/**/*"
  ],
  "keywords": [
    "typescript",
    "tsd",
    "easy",
    "check",
    "checker",
    "test",
    "typings",
    "types"
  ],
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@jest/globals": "^28.0.0",
    "@tsd/typescript": "^4.5.4",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "babel-jest": "^28.0.0",
    "cspell": "^6.0.0",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "jest": "^28.0.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4"
  },
  "peerDependencies": {
    "@tsd/typescript": "^3.8.3 || ^4.0.7"
  },
  "packageManager": "yarn@3.2.1"
}
