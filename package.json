{
  "name": "tsd-lite",
  "version": "0.5.4",
  "description": "Test your TypeScript types easily",
  "license": "MIT",
  "repository": "https://github.com/mrazauskas/tsd-lite.git",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "build": "tsc",
    "lint": "yarn lint:cspell && yarn lint:eslint && yarn lint:prettier",
    "lint:cspell": "cspell --relative --show-context --show-suggestions",
    "lint:eslint": "eslint . --ext .ts",
    "lint:prettier": "prettier . --write",
    "test": "jest"
  },
  "files": [
    "build/**/*"
  ],
  "keywords": [
    "typescript",
    "tsd",
    "easy",
    "check",
    "checker",
    "test",
    "typings",
    "types"
  ],
  "devDependencies": {
    "@babel/core": "7.18.2",
    "@babel/preset-env": "7.18.2",
    "@babel/preset-typescript": "7.17.12",
    "@jest/globals": "28.1.0",
    "@tsd/typescript": "4.7.3",
    "@typescript-eslint/eslint-plugin": "5.27.0",
    "@typescript-eslint/parser": "5.27.0",
    "babel-jest": "28.1.0",
    "cspell": "6.1.2",
    "eslint": "8.17.0",
    "eslint-config-prettier": "8.5.0",
    "jest": "28.1.0",
    "prettier": "2.6.2",
    "typescript": "4.7.3"
  },
  "peerDependencies": {
    "@tsd/typescript": "^3.8.3 || ^4.0.7"
  },
  "packageManager": "yarn@3.2.1"
}
